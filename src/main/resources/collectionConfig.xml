<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:context="http://www.springframework.org/schema/context"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
					       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						   http://www.springframework.org/schema/context
						   http://www.springframework.org/schema/context/spring-context.xsd">
						   
	<bean class="com.springcore.collections.Emp" name="emp1">
		<property name="empName" value="Sneha"/>
		<property name="empPhones">
			<list>
				<value>9999900000</value>
				<value>9898989898</value>
				<value>9089089089</value>
			</list>
		</property>
		
		<property name="empAddress">
			<set>
				<value>Bangalore</value>
				<value>Mangalore</value>
			</set>
		</property>
		
		<property name="empCourses">
			<map>
				<entry key="Java" value="2 months"/>
				<entry key="Python" value="3 months"/>
				<entry key="Spring"><null/></entry>
			</map>
		</property>
		
		
	
	</bean>
	

</beans>

